{
  "volar.autoCompleteRefs": true,
  "files.associations": {
    "*.js": "javascript",
    "*.vue": "vue"
  },
  "files.eol": "\n",
  "eslint.format.enable": true,
  "[javascript]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  },
  "[vue]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  },
  "[typescript]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  },
  "[html]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  },
  "eslint.lintTask.enable": true,
  "html.validate.styles": false,
  "css.validate": false,
  "scss.validate": false,
  "explorer.confirmDelete": false,
  "editor.tabSize": 2,
  "window.zoomLevel": 1
}